{
  "name": "tree-sitter-cue",
  "version": "0.0.1",
  "description": "Cue grammar for tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test"
  },
  "keywords": [
    "parser",
    "cue",
    "cuelang"
  ],
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.2"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.20.6"
  },
  "tree-sitter": [
    {
      "file-types": [
        "cue"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^(cue)$",
      "scope": "source.cue"
    }
  ]
}
